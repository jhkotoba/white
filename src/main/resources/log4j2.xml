<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<configuration status="WARN">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-5level %logger{36}.%M] %msg%n" />
		</Console>
	</appenders>
	<loggers>
		<!-- <root level="info"> -->
		<root level="debug">
			<appender-ref ref="Console" />
		</root>
	</loggers>
	
</configuration>

